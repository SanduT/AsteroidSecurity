<% include partials/header.ejs %>

 <% for(key in asteroids["near_earth_objects"]) { %>

<% asteroids["near_earth_objects"][key].forEach(function(asteroid){ %>
<% if (asteroid["neo_reference_id"] == id) { %>
<h1 style="color:black"> Asteroid name: <%=asteroid["name"]%> </h1>
<h1 style="color:black"> Absolute magnitude: <%=asteroid["absolute_magnitude_h"]%></h1>
<h1 style="color:black">Velocity: <%=asteroid["close_approach_data"][0]["relative_velocity"]["kilometers_per_second"]  %>km/s </h1>
<h1 style="color:black">Estimated diameter: <%=asteroid["estimated_diameter"]["kilometers"]["estimated_diameter_max"] %>km </h1>
<h1 style="color:black">Close approach date: <%= asteroid["close_approach_data"][0]["close_approach_date"] %></h1>
<h1 style="color:black">Miss distance: <%= asteroid["close_approach_data"][0]["miss_distance"]["kilometers"] %>km</h1>
<% if (asteroid["is_potentially_hazardous_asteroid"]) { %>
<h1 style="color:red">The asteroid is potentially harmful </h1>
<% }else { %>
<h1 style="color:green">The asteroid is not harmful </h1>
<% } %>
<% } %>
<% }); %>
<% } %>
<% include partials/footer.ejs %>